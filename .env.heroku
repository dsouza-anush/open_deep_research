# Heroku Environment Configuration
# Set these as config vars in Heroku

# Heroku Inference API (automatically set by Heroku addon)
HEROKU_INFERENCE_API_KEY=
OPENAI_API_BASE=https://api.heroku.com/inference/v1

# Optional: Additional API keys for enhanced functionality
TAVILY_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

# LangSmith (optional for tracing)
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=heroku-deep-research
LANGSMITH_TRACING=false

# Production settings
GET_API_KEYS_FROM_CONFIG=false
PYTHONPATH=/app/src